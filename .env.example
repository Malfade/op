# Пример файла переменных окружения для Railway
# Railway использует эти переменные при развертывании

# Токен Telegram бота
TELEGRAM_TOKEN=

# API ключ Anthropic Claude
ANTHROPIC_API_KEY=

# Модель Claude
CLAUDE_MODEL=claude-3-5-sonnet-20240620

# Уровень логирования (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

# Путь для сохранения логов
LOG_PATH=./logs

# ID пользователей Telegram, разделенные запятыми, которым разрешен доступ
ALLOWED_USERS=

# ID администратора бота
ADMIN_USER_ID= 